{# {{ dump(field.value) }} #}

<div class="form-widget">
  {% for image in field.value %}
      
    <a href="#" class="ea-lightbox-thumbnail" 
    data-featherlight="#ea-lightbox-{{ image.id }}"   data-featherlight-close-on-click="anywhere">
        <img src="/uploads/images/articles/{{ image.image }}" height="100px">
    </a>

    <div id="ea-lightbox-{{ image.id }}" class="ea-lightbox">
        <img src="/uploads/images/articles/{{ image.image }}">
    </div>

        
  {% endfor %} 
</div>



